    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["--model", "gpt-3.5-turbo-0613", "--max_round", "1",  "--group", "2"]
        },
        {
            "name": "Python: Test Project",
            "type": "python",
            "request": "launch",
            "module": "unittest",
            "args": [
                "discover",
                "-s",
                "tests",
                "-p",
                "test_*.py"
            ],
            "console": "integratedTerminal"
        }
    ]
}